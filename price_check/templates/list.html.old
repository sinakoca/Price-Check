<div>WishList:</div>
{% for p, q in wish_list.products %}
<div>
    {{ p.name }} - {{ q }}
    <form action='/update' method=GET>
        <input type='hidden' name='product_id' value='{{ p.id }}'>
        <input type='text' name='quantity' value='{{ q }}' />
        <input type='submit' value='Update' />
    </form>
</div>
{% endfor %}